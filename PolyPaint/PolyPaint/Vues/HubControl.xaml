<UserControl x:Class="PolyPaint.Vues.HubControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PolyPaint.Vues"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="100">
    <StackPanel>
        <Button Content="Galleries" Margin="5" Command="{Binding NavigateGallery}"/>
        <Button Content="Nouvelle image" Margin="5" Command="{Binding NavigateNewSession}"/>
    </StackPanel>
</UserControl>
